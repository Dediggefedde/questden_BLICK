{
"compilerOptions": {
    "target": "ES2020",
		"lib": ["ES2020","DOM"],
    "module": "CommonJS",
    "sourceMap": true,
		"strictNullChecks": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": false,
    "removeComments": false,
    "types": ["Tampermonkey"] // Ensure node types are not included here
  }
}